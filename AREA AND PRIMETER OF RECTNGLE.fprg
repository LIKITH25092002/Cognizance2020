    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="NUMS, VARIABLES, LENGTH, WIDTH, AREA, PERIMETER" type="Integer" array="False" size=""/>
            <output expression="&quot;PLEASE ENTER LENGTH&quot;" newline="True"/>
            <input variable="LENGTH"/>
            <output expression="&quot;PLEASE ENTER WIDTH&quot;" newline="True"/>
            <input variable="WIDTH"/>
