                            <output expression="PERIMETER" newline="True"/>
                        </then>
                        <else>
                            <output expression="&quot;WIDTH MUST BE GREATER THAN ZERO&quot;" newline="True"/>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;LENGTH MUST BE GREATER THAN ZERO&quot;" newline="True"/>
                </else>
            </if>
        </body>
